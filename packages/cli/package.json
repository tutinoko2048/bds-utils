{
  "name": "@bds-utils/cli",
  "version": "0.1.0-beta",
  "type": "module",
  "scripts": {
    "start": "node dist/main.js -c ../../test",
    "build": "bun run scripts/build.ts",
    "compile:dev": "bun build src/main.ts --sourcemap --compile --outfile build/bds-utils",
    "compile:prod": "bun build src/main.ts --minify --compile",
    "compile:linux": "bun run compile:prod --target=bun-linux-x64 --outfile build/bds-utils-linux",
    "compile:windows": "bun run compile:prod --target=bun-windows-x64 --outfile build/bds-utils-windows",
    "typecheck": "tsc --noEmit",
    "dev": "bun src/main.ts -c ../../test --debug"
  },
  "dependencies": {
    "@inquirer/figures": "^1.0.13",
    "@inquirer/prompts": "^7.8.6",
    "@serenityjs/binarystream": "^3.0.10",
    "@serenityjs/nbt": "^0.8.13",
    "commander": "^14.0.1",
    "inquirer-sortable-checkbox": "^1.0.1",
    "jsonc-parser": "^3.3.1",
    "jszip": "^3.10.1",
    "picocolors": "^1.1.1"
  },
  "devDependencies": {
    "@bds-utils/updater-core": "workspace:*"
  },
  "patchedDependencies": {
    "inquirer-sortable-checkbox@1.0.1": "patches/inquirer-sortable-checkbox@1.0.1.patch"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/tutinoko2048/bds-utils.git",
    "directory": "packages/cli"
  },
  "bin": {
    "bds-utils": "./dist/main.js"
  },
  "description": "Utility CLI tool for Bedrock Dedicated Server",
  "engines": {
    "node": ">=20.0.0"
  },
  "keywords": [
    "minecraft",
    "bedrock",
    "server",
    "bds"
  ],
  "files": [
    "dist"
  ]
}
