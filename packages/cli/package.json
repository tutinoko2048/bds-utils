{
  "name": "@bds-utils/cli",
  "description": "Utility CLI tool for Bedrock Dedicated Server",
  "type": "module",
  "version": "0.1.0",
  "bin": {
    "bds-utils": "./dist/main.js"
  },
  "scripts": {
    "start": "node dist/main.js -c ../../test",
    "build": "bun build src/main.ts --outdir dist --target node --format esm",
    "typecheck": "tsc --noEmit",
    "dev": "bun src/main.ts -c ../../test --debug",
    "prepublishOnly": "bun run typecheck && bun run build"
  },
  "keywords": [
    "minecraft",
    "bedrock",
    "server",
    "bds"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/tutinoko2048/bds-utils.git",
    "directory": "packages/cli"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "dependencies": {
    "@bds-utils/updater-core": "workspace:*",
    "@inquirer/prompts": "^7.8.3",
    "@serenityjs/binarystream": "^3.0.10",
    "@serenityjs/nbt": "^0.8.10",
    "commander": "^14.0.0",
    "jsonc-parser": "^3.3.1",
    "jszip": "^3.10.1",
    "picocolors": "^1.1.1"
  }
}
